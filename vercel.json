{
    "version": 2,
    "builds": [
      {
        "src": "client/build/**",
        "use": "@vercel/static"
      },
      {
        "src": "server/dist/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "server/dist/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "client/build/$1"
      }
    ]
  }
  